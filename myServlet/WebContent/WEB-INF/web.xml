<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
      version="3.0">
<!-- Specify which file gets called if no direct file was stated e.g. localhost/dir/ -->
  <welcome-file-list>
    <welcome-file>Start</welcome-file>
  </welcome-file-list>
	
	<servlet>
		<servlet-name>InsertRating</servlet-name>
		<servlet-class>InsertRating</servlet-class>
	</servlet>

   <servlet-mapping>
      <servlet-name>InsertRating</servlet-name>
      <url-pattern>/InsertRating</url-pattern>
   </servlet-mapping>
   
	<servlet>
		<servlet-name>DeleteCustomer</servlet-name>
		<servlet-class>DeleteCustomer</servlet-class>
	</servlet>

   <servlet-mapping>
      <servlet-name>DeleteCustomer</servlet-name>
      <url-pattern>/DeleteCustomer</url-pattern>
   </servlet-mapping>   
 
	<servlet>
		<servlet-name>GetCustomerProduct</servlet-name>
		<servlet-class>GetCustomerProduct</servlet-class>
	</servlet>

   <servlet-mapping>
      <servlet-name>GetCustomerProduct</servlet-name>
      <url-pattern>/GetCustomerProduct</url-pattern>
   </servlet-mapping>  
   
	<servlet>
		<servlet-name>GetRatingCity</servlet-name>
		<servlet-class>GetRatingCity</servlet-class>
	</servlet>

   <servlet-mapping>
      <servlet-name>GetRatingCity</servlet-name>
      <url-pattern>/GetRatingCity</url-pattern>
   </servlet-mapping>
   
   <servlet>
       <servlet-name>GetPhoneNumber</servlet-name>
       <servlet-class>GetPhoneNumber</servlet-class>
   </servlet>
   
   <servlet-mapping>
       <servlet-name>GetPhoneNumber</servlet-name>
       <url-pattern>/GetPhoneNumber</url-pattern>
   </servlet-mapping>

	<servlet>
		<servlet-name>Start</servlet-name>
		<servlet-class>Start</servlet-class>
	</servlet>

   <servlet-mapping>
      <servlet-name>Start</servlet-name>
      <url-pattern>/Start</url-pattern> 
   </servlet-mapping>  
</web-app>
